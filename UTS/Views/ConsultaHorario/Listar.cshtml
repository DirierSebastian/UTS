@model List<ConsultaHorariosModel>;
@{
    ViewData["Title"] = "Listar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="card col-8">
    <div class="card-header">
        Lista
    </div>
</div>
<div class="card-body">
    <table class="table table-striped bg-white">
        <thead>
            <tr>
                <th>ID</th>
                <th>ClaveDeEmpleado</th>
                <th>Nombre</th>
                <th>Fecha</th>
                <th>InicioDelApartado</th>
                <th>FinDelApartado</th>
                <th>NumeroEdificio</th>
                <th>NombreAula</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(modelItem=>item.ID)</td>
                    <td>@Html.DisplayFor(modelItem=>item.ClaveDeEmpleado)</td>
                    <td>@Html.DisplayFor(modelItem=>item.Nombre)</td>
                    <td>@Html.DisplayFor(modelItem=>item.Fecha)</td>
                    <td>@Html.DisplayFor(modelItem=>item.InicioDelApartado)</td>
                    <td>@Html.DisplayFor(modelItem=>item.FinDelApartado)</td>
                    <td>@Html.DisplayFor(modelItem=>item.NumeroEdificio)</td>
                    <td>@Html.DisplayFor(modelItem=>item.NombreAula)</td>
                </tr>
            }
        </tbody>
    </table>


</div>